<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.dishcovery.mapper.CodeMapper">


    <select id="codeList" resultType="com.spring.dishcovery.entity.CodeVO">

        SELECT CODE_HEAD
             ,CODE
             ,CODE_NAME
             ,PARENT_CODE
             ,USE_YN
             ,CODE_SORT
        FROM CODE_MANAGE
        WHERE CODE_HEAD = #{codeHead}
        ORDER BY CODE_SORT

    </select>


</mapper>
